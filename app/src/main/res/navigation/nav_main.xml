<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/choosingActionsFragment">

    <fragment
        android:id="@+id/choosingActionsFragment"
        android:name="talk.push.faceter.feature.fragments.choosing.ChoosingActionsFragment"
        android:label="fragment_choosing_actions"
        tools:layout="@layout/fragment_choosing_actions" >
        <action
            android:id="@+id/action_choosingActionsFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="talk.push.faceter.feature.fragments.settings.SettingsTransmitterFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_transmitter_settings" />
</navigation>